{
  "name": "palvelutarjotin-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node src/start.js",
    "build": "next build",
    "start": "NODE_ENV=production node src/start.js",
    "lint": "eslint --ext js,ts,tsx src",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml dotenv_config_path=.env",
    "format:code": "eslint --ext js,ts,tsx src --fix",
    "test": "jest --watch",
    "test:changed": "jest -o",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:staged": "jest --findRelatedTests",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:local": "BROWSER_TEST_ENV=local yarn browser-test",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" testcafe/e2e/",
    "storybook": "start-storybook -p 9009 -s ./public",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
