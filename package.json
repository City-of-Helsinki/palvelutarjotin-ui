    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "formik": "^2.1.4",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "hds-core": "^0.13.2",
    "hds-react": "^0.13.2",
    "i18next-intervalplural-postprocessor": "^2.0.1",
    "ics": "^2.22.1",
