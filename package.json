    "jest": "^29.4.3",
    "jest-axe": "^6.0.0",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^28.1.0",
    "jest-localstorage-mock": "^2.4.18",
    "jest_workaround": "^0.1.14",
    "lint-staged": "^11.0.0",
    "msw": "^0.35.0",
    "next-page-tester": "^0.33.0",
    "next-server": "^9.0.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.3.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unused-selectors": "^1.0.35",
    "stylelint-scss": "^3.19.0",
    "testcafe": "^2.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4.3",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write",
      "yarn test:staged"
    ]
  }
