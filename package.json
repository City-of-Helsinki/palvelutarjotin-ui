{
  "name": "palvelutarjotin-ui",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "prepare": "husky install",
    "dev": "node src/start.mjs",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node src/start.mjs",
    "lint": "eslint --ext js,ts,tsx src",
    "codegen": "cross-env DOTENV_CONFIG_PATH=\"./.env.local\" graphql-codegen -r dotenv/config --config codegen.yml",
    "format:code": "eslint --ext js,ts,tsx src --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "cross-env SASS_PATH=./src/assets/styles jest --watch",
    "test:changed": "jest -o",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:staged": "cross-env SASS_PATH=./src/assets/styles jest --findRelatedTests",
    "ci": "jest --verbose --coverage --runInBand",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report testcafe/e2e/",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/browser": "^7.53.1",
    "@types/file-saver": "^2.0.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.6",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "formik": "^2.4.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "hds-core": "^2.15.0",
    "hds-react": "^2.15.0",
    "html-react-parser": "^3.0.16",
    "i18next": "^22.5.0",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "ics": "^3.1.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "next": "^13.4.4",
    "next-i18next": "^13.2.2",
    "next-with-apollo": "^5.3.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-helsinki-headless-cms": "^1.0.0-alpha148",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.5",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "sass": "^1.62.1",
    "use-deep-compare-effect": "^1.8.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@faker-js/faker": "^8.0.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@swc/core": "^1.3.61",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/testcafe": "^5.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/classnames": "^2.3.1",
    "@types/dompurify": "^3.0.2",
    "@types/jest": "^29.5.1",
    "@types/jest-axe": "^3.5.5",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-table": "^7.7.14",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-eslint": "^10.1.0",
    "dequal": "^2.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest_workaround": "^0.76.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "next-page-tester": "^0.33.0",
    "next-server": "^9.0.5",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-no-unused-selectors": "^1.0.40",
    "stylelint-scss": "^5.0.0",
    "testcafe": "^2.6.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  }
}
