{
  "name": "palvelutarjotin-ui",
  "version": "2.12.3",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_DISABLE_SENTRY=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 next build",
    "start": "next start",
    "lint": "eslint src",
    "codegen": "cross-env DOTENV_CONFIG_PATH=\"./.env.local\" graphql-codegen -r dotenv/config --config codegen.yml",
    "format:code": "eslint src --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "cross-env SASS_PATH=./src/assets/styles jest --watch",
    "test:changed": "jest -o",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:staged": "cross-env SASS_PATH=./src/assets/styles jest --findRelatedTests --passWithNoTests",
    "ci": "jest --verbose --coverage --runInBand"
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/browser": "^7.53.1",
    "@types/file-saver": "^2.0.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.6",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.2.2",
    "dompurify": "^3.2.4",
    "file-saver": "^2.0.5",
    "formik": "^2.4.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "hds-react": "^3.11.0",
    "html-react-parser": "^3.0.16",
    "i18next": "^22.5.0",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "ics": "^3.1.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "next": "^13.4.4",
    "next-i18next": "^13.2.2",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-helsinki-headless-cms": "1.0.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.5",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "sass": "^1.62.1",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "^11.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@faker-js/faker": "^8.0.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@next/eslint-plugin-next": "^15.2.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@swc/core": "^1.3.61",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/classnames": "^2.3.1",
    "@types/dompurify": "^3.0.2",
    "@types/jest": "^29.5.1",
    "@types/jest-axe": "^3.5.5",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-table": "^7.7.14",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "dequal": "^2.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-js": "eslint/js",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest_workaround": "^0.76.0",
    "lint-staged": "^15.5.0",
    "msw": "^1.2.1",
    "next-server": "^9.0.5",
    "prettier": "^3.5.3",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^16.16.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-no-unused-selectors": "^1.0.40",
    "stylelint-scss": "^6.11.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.2",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  }
}
