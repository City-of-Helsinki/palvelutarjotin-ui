{
  "name": "palvelutarjotin-ui",
  "version": "2.12.3",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_DISABLE_SENTRY=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 next build",
    "start": "next start",
    "lint": "eslint src",
    "codegen": "cross-env DOTENV_CONFIG_PATH=\"./.env.local\" graphql-codegen -r dotenv/config --config codegen.yml",
    "format:code": "eslint src --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "cross-env SASS_PATH=./src/assets/styles jest --watch",
    "test:changed": "jest -o",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:staged": "cross-env SASS_PATH=./src/assets/styles jest --findRelatedTests --passWithNoTests",
    "ci": "jest --verbose --coverage --runInBand"
  },
  "//": {
    "Why are packages in resolutions?": {
      "redux": [
        "To make @reduxjs/toolkit and redux related packages use single version of redux",
        "See https://redux.js.org/usage/migrations/migrating-rtk-2#overriding-dependencies"
      ]
    }
  },
  "resolutions": {
    "redux": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/browser": "^7.120.3",
    "@types/file-saver": "^2.0.7",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "dompurify": "^3.2.4",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "hds-react": "^3.12.0",
    "i18next": "^22.5.1",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "ics": "^3.8.1",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "next": "^13.5.11",
    "next-i18next": "^13.3.0",
    "query-string": "^7.1.3",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.3.1",
    "react-helsinki-headless-cms": "1.0.1",
    "react-i18next": "^12.3.1",
    "react-redux": "^9.2.0",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.6.0",
    "sass": "^1.86.0",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@faker-js/faker": "^9.6.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@next/eslint-plugin-next": "^15.2.4",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@swc/core": "^1.11.13",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/classnames": "^2.3.4",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.28",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@types/react-redux": "^7.1.34",
    "@types/react-table": "^7.7.20",
    "@types/yup": "^0.32.0",
