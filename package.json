{
  "name": "palvelutarjotin-ui",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "node src/start.js",
    "build": "next build",
    "start": "NODE_ENV=production node src/start.js",
    "lint": "eslint --ext js,ts,tsx src",
    "codegen": "DOTENV_CONFIG_PATH=\"./.env.local\" graphql-codegen -r dotenv/config --config codegen.yml",
    "format:code": "eslint --ext js,ts,tsx src --fix",
    "test": "SASS_PATH=./src/assets/styles jest --watch",
    "test:changed": "jest -o",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:staged": "SASS_PATH=./src/assets/styles jest --findRelatedTests",
    "ci": "jest --verbose --coverage --runInBand",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report testcafe/e2e/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@datapunt/matomo-tracker-react": "^0.4.0",
    "@datepicker-react/hooks": "2.3.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@sentry/browser": "^5.19.0",
    "@types/file-saver": "^2.0.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.16.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "formik": "^2.1.4",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "hds-core": "^0.25.0",
    "hds-react": "^0.25.0",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "ics": "^2.22.1",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "next-i18next": "^8.5.1",
    "next-with-apollo": "^5.0.1",
    "query-string": "^6.13.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.0",
    "react-table": "^7.2.2",
    "react-toastify": "^6.0.8",
    "sass": "^1.35.1",
    "use-deep-compare-effect": "^1.3.1",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/testcafe": "^4.4.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/classnames": "^2.2.10",
    "@types/faker": "^5.1.0",
    "@types/jest": "^26.0.0",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.35",
    "@types/react-redux": "^7.1.9",
    "@types/react-table": "^7.0.19",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "core-js": "^2.6.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-testcafe": "^0.2.1",
    "faker": "^5.1.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.5",
    "jest-axe": "^5.0.1",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^10.4.0",
    "next-server": "^9.0.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unused-selectors": "^1.0.35",
    "stylelint-scss": "^3.19.0",
    "testcafe": "^1.14.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write",
      "yarn test:staged"
    ]
  }
}
