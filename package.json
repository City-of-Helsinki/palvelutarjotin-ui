    "@sentry/browser": "^6.8.0",
    "@types/file-saver": "^2.0.1",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.16.1",
    "dompurify": "^2.3.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "formik": "^2.1.4",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "hds-core": "^2.8.0",
    "hds-react": "2.8.0",
    "html-react-parser": "^1.2.8",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "ics": "^2.35.0",
    "jsdom": "^17.0.0",
    "lodash": "^4.17.21",
    "next": "^12.0.9",
    "next-i18next": "^8.5.1",
    "next-with-apollo": "^5.3.0",
    "query-string": "^7.0.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.0.3",
    "react-dom": "^18.2.0",
    "react-helsinki-headless-cms": "1.0.0-alpha37",
    "react-redux": "^7.2.0",
    "react-table": "^7.2.2",
    "react-toastify": "^7.0.4",
    "react-use": "^17.3.1",
    "sass": "^1.35.1",
    "use-deep-compare-effect": "^1.3.1",
    "yup": "^0.32.9"
  },
